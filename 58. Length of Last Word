class Solution {
public:
    int lengthOfLastWord(string s) {
       int count=0;
       int length=0;
       for(int i=0;i<s.size();i++){
           if(s[i]==' '){
               count=0;
           }else{
               count++;
               length=count;
           }
       } 
       return length;
    }
};
